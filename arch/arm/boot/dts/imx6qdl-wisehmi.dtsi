			hactive = <1280>;
			vactive = <480>;
			hback-porch = <268>;
			hfront-porch = <70>;
			vback-porch = <10>;
			vfront-porch = <10>;
			hsync-len = <70>;
			vsync-len = <25>;
		};
		AUO_C123HAN01_1 { /*auo panel*/
			clock-frequency = <95000000>;
			hactive = <1920>;
			vactive = <720>;
			hback-porch = <50>;
			hfront-porch = <50>;
			vback-porch = <15>;
			vfront-porch = <15>;
			hsync-len = <60>;
			vsync-len = <10>;
		};
        CLAA123PA01CW { /*cpt 1440*540 panel*/
            clock-frequency = <61250000>;
            hactive = <1440>;
            vactive = <540>;
            hback-porch = <200>;
            hfront-porch = <200>;
            vback-porch = <6>;
            vfront-porch = <6>;
